<%- contentFor('body') %>
<% const role = {} %>
<div class="row">
<div class="col-md">
    <div class="card card-primary">
      <div class="card-header">
        <h3 class="card-title">Department</h3>

        <div class="card-tools">
          <button type="button" class="btn btn-tool" data-card-widget="collapse" data-toggle="tooltip" title="Collapse">
            <i class="fas fa-minus"></i></button>
        </div>
      </div>
      <div class="card-body">
        <form action="" method="post">
          
          <div class="form-group">
          <label for="attributeName">Attribute Name</label>
          <input name="attributeName" type="text" id="attributeName" value="<%= attribute.name || '' %>" required="required" class="form-control">
        </div>

        <div class="form-group">
          <label for="attributeType">Attribute Type</label>
          <input name="attributeType" readonly type="text" id="attributeType" value="Text" readonly required="required" class="form-control">
        </div>
        <div class="form-group">
          <label for="attributeType">Defualt Value</label>
          <input name="attributeDefault" type="text" id="attributeDefault" value="" required="required" class="form-control">
        </div>
        

        </div>


        <div class="form-group">
          <input type="submit" value="Add Job Title" class="btn btn-success float-right">
        </div>
        </form>
      </div>
      <!-- /.card-body -->
    </div>
    <!-- /.card -->
  </div>


<%-contentFor('styles')%>


<%-contentFor('scripts')%>