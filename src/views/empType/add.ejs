<%- contentFor('body') %>

<div class="row">
  <div class="col-md">
    <div class="card card-primary">
      <div class="card-header">
        <h3 class="card-title">Employment Type</h3>

        <div class="card-tools">
          <button type="button" class="btn btn-tool" data-card-widget="collapse" data-toggle="tooltip" title="Collapse">
            <i class="fas fa-minus"></i></button>
        </div>
      </div>
      <div class="card-body">
        <form action="/empType/add" enctype="application/x-www-form-urlencoded" method="post">
        <div class="form-group">
          <label for="inputType">Employment</label>
          <input name="employmentType" type="text" id="inputType" value="<%=empType.type || ''%>" required="required" class="form-control">
        </div>

        <div class="form-group">
          <input type="submit" value="Create Employment Type" class="btn btn-success float-right">
        </div>
        </form>
      </div>
      <!-- /.card-body -->
    </div>
    <!-- /.card -->
  </div>
</div>

<%-contentFor('styles')%>


<%-contentFor('scripts')%>